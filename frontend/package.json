  "scripts": {
    "start": "webpack serve --mode development --port=3000",
    "prebuild:dev1": "rm -rf build && yarn install",
    "prebuild:qa": "rm -rf build && yarn install",
    "prebuild:stag": "rm -rf build && yarn install",
    "prebuild:prod": "rm -rf build && yarn install",
    "build:dev1": "export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); env-cmd -f .env.dev webpack --mode development && node sentry.js",
    "build:qa": "export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); env-cmd -f .env.qa webpack && node sentry.js",
    "build:stag": "env-cmd -f .env.stag webpack && node sentry.js",
    "build:prod": "env-cmd -f .env.prod webpack && node sentry.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "test": "jest --env=node --colors test --passWithNoTests",
    "test:cov": "jest --env=node --colors --coverage test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
